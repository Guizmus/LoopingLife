<?xml version="1.0"?>
<body>
  <meta>
    <title>Looping Life</title>
  </meta>
  <menu>
    <buttons>
      <save>Sauvegarder</save>
      <load>Charger</load>
    </buttons>
  </menu>
  <stages>
    <stage_1>
        <libel>Nourisson</libel>
    </stage_1>
    <stage_2>
        <libel>Enfant</libel>
    </stage_2>
  </stages>
  <resources>
    <title>Ressources</title>
    <titleInfo>Ressources actuelles. Attention, certaines limites une fois atteintes ne peuvent être oubliées...</titleInfo>
    <lifeMater>
      <libel>Essence vitale</libel>
    </lifeMater>
    <enjoyment>
      <libel>Joie</libel>
    </enjoyment>
  </resources>
  <statistics>
    <stamina>
      <libel>Endurance</libel>
    </stamina>
    <imagination>
      <libel>Imagination</libel>
    </imagination>
    <charisma>
      <libel>Charisme</libel>
    </charisma>
  </statistics>
  <actions>
    <buttons>
      <queue>Ajouter à la liste</queue>
    </buttons>
    <titles>
      <actionList>Tâches à effectuer</actionList>
      <actionListInfo>La liste des choses à faire. Préparez la suite, ou bien planifiez les prochaines boucles ici.</actionListInfo>
    </titles>
    <stage_1>
      <changeStage>
        <libel>Grandir</libel>
        <info>Il est temps de passer de 4 à 2 pattes</info>
      </changeStage>
      <crying>
        <libel>Pleurer</libel>
        <info>La tristesse est partie intégrante de la vie</info>
      </crying>
      <playing>
        <libel>Jouer</libel>
        <info>Le carré va dans le carré</info>
      </playing>
      <resting>
        <libel>Dormir</libel>
        <info>Le meilleur moment de la journée</info>
      </resting>
    </stage_1>
    <stage_2>
      <resting>
        <libel>Dormir</libel>
        <info>Passez une bonne nuit</info>
      </resting>
    </stage_2>
  </actions>
</body>
